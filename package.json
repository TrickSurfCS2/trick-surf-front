  "name": "trick-surf",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "contributors": [
    "Ivan Kornilov"
  ],
  "packageManager": "bun@1.0.14",
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.x"
  },
  "scripts": {
    "dev": "vite",
